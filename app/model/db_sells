from peewee import *
from db_models import BaseModel

class TB_Vendas(BaseModel):
    NomeCliente = CharField()
    Produto = CharField()
    Qtde = IntegerField()
    ValorTotal = FloatField()
    Situacao = CharField()
    DataVenda = DateTimeField()

if __name__ == '__main__':
    TB_Vendas.create_table()
